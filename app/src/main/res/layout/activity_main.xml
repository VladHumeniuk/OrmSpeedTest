<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="orm.test.ormtest.MainActivity">

    <Button
        android:id="@+id/electra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Electra"/>

    <Button
        android:id="@+id/dbflow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DB Flow"/>

    <Button
        android:id="@+id/cupboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cupboard"/>

    <Button
        android:id="@+id/requery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Requery"/>

    <Button
        android:id="@+id/cipher_electra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cipher+Electra"/>

    <Button
        android:id="@+id/realm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Realm"/>

    <Button
        android:id="@+id/realm_encrypted"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Realm encrypted"/>
</LinearLayout>
